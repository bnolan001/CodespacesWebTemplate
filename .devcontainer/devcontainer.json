{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ritwickdey.LiveServer",
        "esbenp.prettier-vscode",
        "vsls-contrib.gitdoc"
      ],
      "settings":{
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "gitdoc.commitValidationLevel": "none",
        "gitdoc.enabled": true
      }
    }
  }
}
